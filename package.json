{
  "name": "@public/statistiques",
  "version": "0.0.1",
  "description": "A package to interract with the Discord API coded by myself",
  "main": "build/server",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && npm run run",
    "run": "node .",
    "dev": "nodemon --watch src --ext ts --exec npm run dev:serve",
    "dev:serve": "ts-node --files src/server.ts true",
    "test": "nodemon --watch tests --ext ts --exec npm run test:serve",
    "test:serve": "ts-node --files tests/index.ts true",
    "prettier": "prettier **.json src tests --write",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase": "^9.8.1",
    "body-parser": "^1.20.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "contributors": [
    "fantomitechno <fantomitechno@gmail.com>"
  ],
  "author": "fantomitechno <fantomitechno@gmail.com>",
  "license": "ISC",
  "type": "module"
}
